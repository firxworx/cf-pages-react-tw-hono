{
  "name": "cf-pages-react-tw-hono",
  "type": "module",
  "packageManager": "pnpm@8.7.1",
  "scripts": {
    "dev": "vite",
    "build": "NODE_ENV=production vite build --mode client && NODE_ENV=production vite build",
    "build:css": "tailwindcss -i ./src/style.css -o ./dist/static/style.css",
    "preview": "wrangler pages dev dist",
    "deploy:cloudflare": "$npm_execpath run build && wrangler pages deploy dist",
    "biome:format": "biome format .",
    "biome:lint": "biome lint .",
    "biome:check": "biome check .",
    "biome:format:write": "biome format --write .",
    "biome:lint:write": "biome lint --write .",
    "biome:check:write": "biome check --write .",
    "reset": "pnpm dlx rimraf --glob ./**/node_modules && pnpm dlx rimraf --glob ./**/dist",
    "kill:vscode-server": "ps uxa | grep .vscode-server | awk '{print $2}' | xargs kill"
  },
  "dependencies": {
    "hono": "^4.3.11",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "@cloudflare/workers-types": "^4.20240208.0",
    "@csstools/postcss-oklab-function": "^3.0.19",
    "@hono/vite-cloudflare-pages": "^0.4.0",
    "@hono/vite-dev-server": "^0.12.1",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.13",
    "@types/react": "^18.2.60",
    "@types/react-dom": "^18.2.19",
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.39",
    "postcss-discard-comments": "^7.0.1",
    "tailwindcss": "^3.4.6",
    "vite": "^5.2.11",
    "vite-tsconfig-paths": "^4.3.2",
    "wrangler": "^3.57.1"
  }
}
